<SigMonD>
  <Initialize>
    <ProjectName>pro-jekt</ProjectName>
    <LogFile>good-filename.log</LogFile>
    <EchoXML />
    <MCObservables>
      <MCEnsembleInfo>clover_s32_t256_ud860_s743</MCEnsembleInfo>
      <CorrelatorData>
        <FileListInfo>
          <FileNameStub>test_data/test</FileNameStub>
          <MinFileNumber>0</MinFileNumber>
          <MaxFileNumber>15</MaxFileNumber>
        </FileListInfo>
      </CorrelatorData>
      <Specifications>
        <HermitianCorrelationMatrix>
          <AssignName>pro-jekt</AssignName>
          <OperatorString>a</OperatorString>
          <OperatorString>b</OperatorString>
          <OperatorString>c</OperatorString>
          <OperatorString>d</OperatorString>
          <OperatorString>e</OperatorString>
          <OperatorString>f</OperatorString>
          <OperatorString>g</OperatorString>
          <OperatorString>h</OperatorString>
          <OperatorString>i</OperatorString>
          <OperatorString>j</OperatorString>
          <OperatorString>k</OperatorString>
          <OperatorString>l</OperatorString>
          <OperatorString>m</OperatorString>
          <OperatorString>n</OperatorString>
          <OperatorString>o</OperatorString>
          <OperatorString>p</OperatorString>
        </HermitianCorrelationMatrix>
      </Specifications>
    </MCObservables>
    <Bootstrapper>
      <NumberResamplings>1024</NumberResamplings>
      <Seed>6754</Seed>
      <BootSkip>127</BootSkip>
      <Precompute />
    </Bootstrapper>
  </Initialize>
  <TaskSequence>
    <Task>
      <Action>DoCorrMatrixRotation</Action>
      <MinTimeSep>3</MinTimeSep>
      <MaxTimeSep>26</MaxTimeSep>
      <Type>SinglePivot</Type>
      <SinglePivotInitiate>
        <RotationName>pro-jekt</RotationName>
        <AssignName>Pivpro-jekt</AssignName>
        <CorrelatorMatrixInfo>
          <Name>pro-jekt</Name>
        </CorrelatorMatrixInfo>
        <NormTime>3</NormTime>
        <MetricTime>4</MetricTime>
        <DiagonalizeTime>6</DiagonalizeTime>
        <MinimumInverseConditionNumber>1e-3</MinimumInverseConditionNumber>
        <WriteToFile>
          <Filename>SinglePivot_isospin_etc</Filename>
          <Overwrite />
        </WriteToFile>
      </SinglePivotInitiate>
    </Task>
    <Task>
      <Action>DoCorrMatrixRotation</Action>
      <MinTimeSep>3</MinTimeSep>
      <MaxTimeSep>26</MaxTimeSep>
      <Type>SinglePivot</Type>
      <SinglePivotInitiate>
        <ReadFromFile>
          <Name>SinglePivot_isospin_etc</Name>
        </ReadFromFile>
        <AssignName>Pivpro-jekt</AssignName>
      </SinglePivotInitiate>
      <WriteToFile>
        <Filename>RotatedCorrelators_isospin_etc</Filename>
        <Overwrite />
      </WriteToFile>
    </Task>
    <Task>
      <Action>ReadBinsFromFile</Action>
      <FileName>RotatedCorrelatorsisospin_etc</FileName>
    </Task>
    <Task>
      <Action>DoCorrMatrixRotation</Action>
      <Type>TemporalCorrelator</Type>
      <MinimizerInfo>
        <Method>Minuit2</Method>
        <ParameterRelTol>1e-6</ParameterRelTol>
        <ChiSquareRelTol>1e-4</ChiSquareRelTol>
        <MaximumIterations>1024</MaximumIterations>
        <Verbosity>Low</Verbosity>
      </MinimizerInfo>
      <SamplingMode>Bootstrap</SamplingMode>
      <TemporalCorrelatorFit>
        <RotatedOperator>
          <ObsName>proj_name</ObsName>
          <Level>1</Level>
        </RotatedOperator>
        <MinimumTimeSeparation>6</MinimumTimeSeparation>
        <MaximumTimeSeparation>25</MaximumTimeSeparation>
        <LargeTimeNoiseCutoff>1.0</LargeTimeNoiseCutoff>
        <Model>
          <Type>TimeSymGeomSeriesExponential</Type>
          <Energy>
            <Name>FitSE</Name>
            <IDIndex>0</IDIndex>
          </Energy>
          <Amplitude>
            <Name>A0SE</Name>
            <IDIndex>0</IDIndex>
          </Amplitude>
          <FirstEnergy>
            <Name>FitTE</Name>
            <IDIndex>0</IDIndex>
          </FirstEnergy>
          <FirstAmplitude>
            <Name>A0TE</Name>
            <IDIndex>0</IDIndex>
          </FirstAmplitude>
          <SqrtGapToSecondEnergy>
            <Name>FitGapTE</Name>
            <IDIndex>0</IDIndex>
          </SqrtGapToSecondEnergy>
          <SecondAmplitudeRatio>
            <Name>A1TE</Name>
            <IDIndex>0</IDIndex>
          </SecondAmplitudeRatio>
          <AddedConstant>
            <Name>C</Name>
            <IDIndex>0</IDIndex>
          </AddedConstant>
        </Model>
        <DoEffectiveEnergyPlot>
          <PlotFile>plot.agr</PlotFile>
          <CorrName>Particle.energy</CorrName>
          <TimeStep>3</TimeStep>
          <SymbolColor>blue</SymbolColor>
          <SymbolType>circle</SymbolType>
          <Goodness>chisq</Goodness>
          <ShowApproach />
        </DoEffectiveEnergyPlot>
      </TemporalCorrelatorFit>
    </Task>
    <Task>
      <Action>DoChecks</Action>
      <Type>TemporalCorrelatorMatrix</Type>
      <CorrelatorMatrixInfo>
        <Name>pro-jekt</Name>
      </CorrelatorMatrixInfo>
      <MinTimeSep>3</MinTimeSep>
      <MaxTimeSep>26</MaxTimeSep>
      <OutlierScale>20</OutlierScale>
      <Verbose />
    </Task>
    <Task>
      <Action>DoChecks</Action>
      <Type>TemporalCorrelatorMatrixIsHermitian</Type>
      <CorrelatorMatrixInfo>
        <Name>pro-jekt</Name>
      </CorrelatorMatrixInfo>
      <MinTimeSep>3</MinTimeSep>
      <MaxTimeSep>26</MaxTimeSep>
      <Verbose />
    </Task>
  </TaskSequence>
</SigMonD>
